{
  "name": "@cenz/string-lib",
  "version": "1.1.3",
  "description": "A lightweight library that adds more functionality to Javascript's built in type, String",
  "main": "umd/index.js",
  "module": "esm/index.js",
  "files": [
    "esm/*",
    "umd/*"
  ],
  "scripts": {
    "test": "npm run prepare && mocha test.js",
    "prepare": "rollup --config"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/C3NZ/string-lib.git"
  },
  "author": "C3NZ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/C3NZ/string-lib/issues"
  },
  "homepage": "https://github.com/C3NZ/string-lib#readme",
  "devDependencies": {
    "husky": "^3.0.1",
    "mocha": "^10.2.0",
    "rollup": "^1.17.0",
    "rollup-plugin-terser": "^5.1.1"
  }
}
